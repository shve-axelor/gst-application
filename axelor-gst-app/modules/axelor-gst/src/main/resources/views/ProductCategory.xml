<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<grid model="com.axelor.gst.db.ProductCategory" title="ProductCategories"
		name="productcategory-grid">
		<field name="productCategoryName" required="true" />
			<field name="productCategorycode" required="true" />
			<field name="gstRate" />
	</grid>

	<form model="com.axelor.gst.db.ProductCategory" title="ProductCategories"
		name="productcategory-form">
		<panel title="Product Category Details">
			<field name="productCategoryName" required="true" />
			<field name="productCategorycode" required="true" />
			<field name="gstRate" />
		</panel>
	</form>

</object-views>